<?xml version="1.0" encoding="UTF-8"?>
  <!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.bridgelabz.main.Employee" table="employee_details">
		<id type="int" name="id">
			<generator class="assigned"></generator>
		</id>
		<property type="java.lang.String" name="firstName">
		</property>
		<property type="java.lang.String" name="lastName"></property>
		<property type="java.lang.String" name="contactNo"></property>

	</class>
</hibernate-mapping> 